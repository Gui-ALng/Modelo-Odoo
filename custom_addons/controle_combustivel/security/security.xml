<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="group_fuel_driver" model="res.groups">
            <field name="name">Motorista</field>
        </record>

        <record id="group_fuel_analyst" model="res.groups">
            <field name="name">Analista</field>
        </record>

        <record id="group_fuel_manager" model="res.groups">
            <field name="name">Administrador</field>
            <field name="implied_ids" eval="[(4, ref('group_fuel_driver')), (4, ref('group_fuel_analyst'))]"/>
        </record>
    </data>
</odoo>